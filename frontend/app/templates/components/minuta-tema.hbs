<div class="temaContent flex-center space-between white">
  <div class="mr-20" style="width:430px;">
    <td class="noPadding" {{action 'verEditorDeConclusion' temaDeMinuta}}>
      <h5 class="titulo-de-tema">{{temaDeMinuta.tema.titulo}}</h5>
      <p class="crop conclusion" >{{markdown-view
        value=temaDeMinuta.tema.descripcion}}</p>
    </td>
  </div>

  <divW>
    <h6 class="center-align"> Â¿Fue tratado?</h6>
    <div class="space-between mt-20">
      <button type="button" {{action ''}} class="btn-flat bold btn mr-20">
        <h6 class="bold"> SI</h6>
      </button>
      <button type="button" {{action ''}} class="btn-flat bold">
        <h6 class="bold"> NO </h6>
      </button>
    </div>
  </divW>

</div>

<div id="mostrar-edito">
  {{# if mostrandoEditor}}
    {{editor-de-conclusion
      alGuardar=(action 'guardarConclusion')
      alCerrar=(action 'cerrarEditor')
      temaDeMinuta=temaDeMinuta
      usuarios=model.usuarios
    }}
  {{/if}}
</div>